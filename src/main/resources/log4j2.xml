<Configuration status="info" packages="com.rapidomize.util.log4j2">
    <Appenders>
        <RapidomizeAppender name="STDOUT" appid="appid" token="some">
            <PatternLayout pattern="%d [%t] %-5p %c{1} - %m"/>
        </RapidomizeAppender>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>